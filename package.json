{
  "name": "anywherepip",
  "version": "1.0.0",
  "description": "ブラウザを前面に表示させる",
  "main": "./src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "npx eslint ./src/*.ts",
    "prettier": "./node_modules/.bin/prettier --write ./src/**/*.ts "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sim-mokomo/anywherePIP.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sim-mokomo/anywherePIP/issues"
  },
  "homepage": "https://github.com/sim-mokomo/anywherePIP#readme",
  "dependencies": {
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^7.25.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.6"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "anywherepip"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
